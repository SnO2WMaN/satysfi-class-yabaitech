    (TOCElementChapter(`5`, {チャプターその5とちょっと眺めのタイトル}, Option.none, {著者の5}), Option.some(`43`));
    (TOCElementChapter(`6`, {チャプターその6}, Option.none, {著者の6}), Option.some(`53`));
    (TOCElementChapter(`7`, {チャプターその7}, Option.none, {著者の7}), Option.some(`63`));
